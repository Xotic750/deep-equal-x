{
  "name": "deep-equal-x",
  "version": "1.4.1",
  "description": "node's deepEqual and deepStrictEqual algorithm.",
  "homepage": "https://github.com/Xotic750/deep-equal-x",
  "author": "Graham Fairweather <xotic750@gmail.com>",
  "copyright": "Copyright (c) 2015-2017",
  "keywords": [
    "es6",
    "deepEqual",
    "deepStrictEqual",
    "library",
    "javascript",
    "testing",
    "nodejs",
    "browser",
    "commonjs",
    "test"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": "https://github.com/Xotic750/deep-equal-x.git",
  "bugs": {
    "url": "https://github.com/Xotic750/deep-equal-x/issues"
  },
  "dependencies": {
    "array.prototype.some": "^1.0.0",
    "index-of-x": "^1.2.0",
    "is-arguments": "^1.0.2",
    "is-buffer": "^1.1.5",
    "is-date-object": "^1.0.1",
    "is-error-x": "^1.2.0",
    "is-map-x": "^1.2.0",
    "is-nil-x": "^1.2.0",
    "is-object": "^1.0.1",
    "is-primitive": "^2.0.0",
    "is-regex": "^1.0.4",
    "is-set-x": "^1.2.0",
    "is-string": "^1.0.4",
    "lodash._arrayfilter": "^3.0.0",
    "lodash._slice": "^2.4.1",
    "lodash.isnull": "^3.0.0",
    "object-keys": "^1.0.11",
    "stable": "^0.1.6",
    "to-string-tag-x": "^1.2.0"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "^11.0.0",
    "browserify": "^14.1.0",
    "browserify-derequire": "^0.9.4",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "es7-shim": "^6.0.0",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^3.0.0",
    "json3": "^3.3.2",
    "make-jasmine-spec-runner-html": "^1.1.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "replace-x": "^1.1.1",
    "uglify-js": "^2.8.18"
  },
  "scripts": {
    "build": "npm run build:setver && npm run security && npm run eslint && npm run browserify && npm run uglify && npm run docs && npm run build:jasmine && npm test",
    "build:jasmine": "make-jasmine-spec-runner-html",
    "build:setver": "PKG_VER=$(node -p -e \"require('./package.json').version\") && replace-x \" @version .*\" \" @version ${PKG_VER}\" index.js",
    "build:watch": "nodemon --watch index.js --watch package.json --exec 'npm run build'",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "eslint": "eslint *.js tests/spec/*.js",
    "security": "nsp check",
    "test": "jasmine-node --matchall tests/spec/",
    "test:watch": "nodemon --watch tests/spec/test.js --exec 'npm test'",
    "browserify": "browserify -p browserify-derequire -e index.js -o lib/deep-equal-x.js -u 'crypto' -s returnExports",
    "uglify": "uglifyjs lib/deep-equal-x.js -o lib/deep-equal-x.min.js --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/deep-equal-x.map"
  }
}
